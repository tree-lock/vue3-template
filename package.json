{
  "name": "xmo-vue3-template",
  "description": "vue3+vite+ts开发模板",
  "homepage": "https://darkxmo.github.io/vue3-template",
  "license": "MIT",
  "version": "1.2.0-beta.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:release": "vue-tsc --noEmit && vite build --mode release",
    "build:template": "vue-tsc --noEmit && vite build --outDir docs --mode template && cp docs/index.html docs/404.html",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts --ext .vue --fix",
    "commit": "git add . && git status && git-cz",
    "prepare": "husky install",
    "release": "npm run build && gulp",
    "version": "gulp customizeVersion"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "*",
    "@types/mockjs": "^1.0.7",
    "@vitejs/plugin-vue": "^3.1.2",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "element-plus": "^2.2.19",
    "js-cookie": "^3.0.1",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "once-init": "^1.0.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "fs-extra": "^10.1.0",
    "git-cz": "^4.9.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "inquirer": "^9.1.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "vue-eslint-parser": "^9.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
